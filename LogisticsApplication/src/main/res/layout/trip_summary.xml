<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:CustomTextView="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/new_back2"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.logimetrix.nj.logistics_project.utills.CustomTextView
        android:id="@+id/trip_summary"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TRIP SUMMARY"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_16"
        android:layout_marginTop="@dimen/dp_20"
        android:layout_marginLeft="@dimen/dp_5"
        android:padding="@dimen/dp_4"
        CustomTextView:textStyle="8"/>
    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_5"
        android:layout_marginTop="@dimen/dp_25"
        android:layout_marginRight="@dimen/dp_5"
        android:id="@+id/card_trip_summary"
        app:cardElevation="@dimen/dp_8">
        <LinearLayout
            android:background="#FFF"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingLeft="@dimen/dp_minus2"
            android:layout_marginTop="@dimen/dp_10"
            android:paddingRight="@dimen/dp_minus2"
            android:paddingTop="@dimen/dp_2">
            <LinearLayout
                android:layout_marginRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_5"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_gravity="center">
                <RelativeLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_weight="7"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/phone"
                        android:layout_width="@dimen/dp_25"
                        android:layout_height="@dimen/dp_25"
                        android:paddingTop="@dimen/dp_8"
                        android:src="@drawable/phone"/>
                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_0"
                        android:layout_toRightOf="@+id/phone"
                        android:layout_marginTop="@dimen/dp_10"
                        android:id="@+id/phone_number"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/dp_10"
                        android:layout_weight="1"
                        CustomTextView:textStyle="3"
                        android:textColor="@color/green"
                        android:text="PHONE NUMBER" />

                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_8"
                        android:paddingLeft="@dimen/dp_22"
                        android:paddingRight="@dimen/dp_22"
                        android:gravity="left"
                        android:textSize="@dimen/dp_10"
                        android:layout_below="@id/phone_number"
                        android:id="@+id/phone_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:text="97146889450"
                        CustomTextView:textStyle="4"/>

                </RelativeLayout>
            </LinearLayout>


            <LinearLayout
                android:layout_marginRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_7"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_gravity="center">
                <RelativeLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_weight="7"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/date"
                        android:layout_width="@dimen/dp_25"
                        android:layout_height="@dimen/dp_25"
                        android:paddingTop="@dimen/dp_8"
                        android:src="@drawable/invoicedate"/>
                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_0"
                        android:layout_toRightOf="@+id/date"
                        android:layout_marginTop="@dimen/dp_8"
                        android:id="@+id/reporting_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/dp_10"
                        android:layout_weight="1"
                        android:textColor="@color/green"
                        android:text="REPORTING DATE"
                        CustomTextView:textStyle="3"
                        android:textStyle="bold"/>

                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_8"
                        android:paddingLeft="@dimen/dp_22"
                        android:paddingRight="@dimen/dp_22"
                        android:gravity="left"
                        android:textSize="@dimen/dp_10"
                        android:layout_below="@id/reporting_date"
                        android:id="@+id/reporting_date_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:text="4 Aug 2017"
                        CustomTextView:textStyle="4"/>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_weight="7"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/time"
                        android:layout_width="@dimen/dp_25"
                        android:layout_height="@dimen/dp_25"
                        android:paddingTop="@dimen/dp_8"
                        android:src="@drawable/time"/>
                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_0"
                        android:layout_toRightOf="@+id/time"
                        android:layout_marginTop="@dimen/dp_8"
                        android:id="@+id/reporting_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/dp_10"
                        android:layout_weight="1"
                        android:textColor="@color/green"
                        android:text="REPORTING TIME"
                        android:textStyle="bold"
                        CustomTextView:textStyle="3"/>

                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_8"
                        android:paddingLeft="@dimen/dp_22"
                        android:paddingRight="@dimen/dp_22"
                        android:gravity="left"
                        android:textSize="@dimen/dp_10"
                        android:layout_below="@id/reporting_time"
                        android:id="@+id/reporting_time_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:text="2:30 PM"
                        CustomTextView:textStyle="4"/>

                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_7"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_gravity="center">
                <RelativeLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_weight="7"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/date1"
                        android:layout_width="@dimen/dp_25"
                        android:layout_height="@dimen/dp_25"
                        android:paddingTop="@dimen/dp_8"
                        android:src="@drawable/invoicedate"/>
                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_0"
                        android:layout_toRightOf="@+id/date1"
                        android:layout_marginTop="@dimen/dp_8"
                        android:id="@+id/unloading_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/dp_10"
                        android:layout_weight="1"
                        android:textColor="@color/green"
                        android:text="UNLOADING DATE"
                        CustomTextView:textStyle="3"
                        android:textStyle="bold"/>

                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_8"
                        android:paddingLeft="@dimen/dp_22"
                        android:paddingRight="@dimen/dp_22"
                        android:gravity="left"
                        android:textSize="@dimen/dp_10"
                        android:layout_below="@id/unloading_date"
                        android:id="@+id/unloading_date_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:text="4 Aug 2017"
                        CustomTextView:textStyle="4"/>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_weight="7"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/time1"
                        android:layout_width="@dimen/dp_25"
                        android:layout_height="@dimen/dp_25"
                        android:paddingTop="@dimen/dp_8"
                        android:src="@drawable/time"/>
                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_0"
                        android:layout_toRightOf="@+id/time1"
                        android:layout_marginTop="@dimen/dp_8"
                        android:id="@+id/unloading_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/dp_10"
                        android:layout_weight="1"
                        android:textColor="@color/green"
                        android:text="UNLOADING TIME"
                        android:textStyle="bold"
                        CustomTextView:textStyle="3"/>

                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_8"
                        android:paddingLeft="@dimen/dp_22"
                        android:paddingRight="@dimen/dp_22"
                        android:gravity="left"
                        android:textSize="@dimen/dp_10"
                        android:layout_below="@id/unloading_time"
                        android:id="@+id/unloading_time_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:text="2:30 PM"
                        CustomTextView:textStyle="4"/>

                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginRight="@dimen/dp_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_7"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_gravity="center">
                <RelativeLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_weight="7"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/date2"
                        android:layout_width="@dimen/dp_25"
                        android:layout_height="@dimen/dp_25"
                        android:paddingTop="@dimen/dp_8"
                        android:src="@drawable/invoicedate"/>
                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_0"
                        android:layout_toRightOf="@+id/date2"
                        android:layout_marginTop="@dimen/dp_8"
                        android:id="@+id/out_date"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/dp_10"
                        android:layout_weight="1"
                        android:textColor="@color/green"
                        android:text="OUT DATE"
                        android:textStyle="bold"
                        CustomTextView:textStyle="3"/>

                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_8"
                        android:paddingLeft="@dimen/dp_22"
                        android:paddingRight="@dimen/dp_22"
                        android:gravity="left"
                        android:textSize="@dimen/dp_10"
                        android:layout_below="@id/out_date"
                        android:id="@+id/out_date_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:text="4 Aug 2017"
                        CustomTextView:textStyle="4"/>

                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_weight="7"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/time2"
                        android:layout_width="@dimen/dp_25"
                        android:layout_height="@dimen/dp_25"
                        android:paddingTop="@dimen/dp_8"
                        android:src="@drawable/time"/>
                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_0"
                        android:layout_toRightOf="@+id/time2"
                        android:layout_marginTop="@dimen/dp_8"
                        android:id="@+id/out_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/dp_10"
                        android:layout_weight="1"
                        android:textColor="@color/green"
                        android:text="OUT TIME"
                        android:textStyle="bold"
                        CustomTextView:textStyle="3"/>

                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_8"
                        android:paddingLeft="@dimen/dp_22"
                        android:paddingRight="@dimen/dp_22"
                        android:gravity="left"
                        android:textSize="@dimen/dp_10"
                        android:layout_below="@id/out_time"
                        android:id="@+id/out_time_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:text="2:30 PM"
                        CustomTextView:textStyle="4"/>

                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_marginRight="@dimen/dp_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_7"
                android:layout_marginBottom="@dimen/dp_15"
                android:layout_marginLeft="@dimen/dp_20"
                android:layout_gravity="center">
                <RelativeLayout
                    android:layout_width="@dimen/dp_0"
                    android:layout_weight="7"
                    android:layout_height="match_parent">
                    <ImageView
                        android:id="@+id/packages"
                        android:layout_width="@dimen/dp_25"
                        android:layout_height="@dimen/dp_25"
                        android:paddingTop="@dimen/dp_8"
                        android:src="@drawable/pckng"/>
                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_0"
                        android:layout_toRightOf="@+id/packages"
                        android:layout_marginTop="@dimen/dp_8"
                        android:id="@+id/pack"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/dp_10"
                        android:layout_weight="1"
                        android:textColor="@color/green"
                        android:text="NUMBER OF PACKAGES RECEIVED"
                        android:textStyle="bold"
                        CustomTextView:textStyle="3"/>

                    <com.logimetrix.nj.logistics_project.utills.CustomTextView
                        android:paddingTop="@dimen/dp_8"
                        android:paddingLeft="@dimen/dp_22"
                        android:paddingRight="@dimen/dp_22"
                        android:gravity="left"
                        android:textSize="@dimen/dp_10"
                        android:layout_below="@id/pack"
                        android:id="@+id/packages_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="#000"
                        android:text="40"
                        CustomTextView:textStyle="4"/>

                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </android.support.v7.widget.CardView>
</LinearLayout>